{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "intervention/image": "^3.11",
        "laravel-notification-channels/webpush": "*",
        "laravel/framework": "^12.0",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.13",
        "laravel/tinker": "^2.10.1",
        "predis/predis": "^3.1",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-json-api-paginate": "^2.0",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-permission": "^6.18",
        "spatie/laravel-query-builder": "^6.3",
        "symfony/dom-crawler": "^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.3",
        "laravel/boost": "^1.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.27",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve --host=0.0.0.0\" \"php artisan queue:listen --tries=1\" \"php artisan reverb:start --debug\" --names=server,queue,reverb"
        ],
        "dev:logs": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan pail --timeout=0"
        ],
        "dev:all": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve --host=0.0.0.0\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan reverb:start --debug\" --names=server,queue,logs,reverb"
        ],
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage/html --coverage-clover coverage/clover.xml --coverage-text=coverage/coverage.txt",
        "test:coverage-check": "php scripts/check-coverage.php",
        "test:coverage-full": [
            "@test:coverage",
            "@test:coverage-check"
        ],
        "test:fix": "php scripts/fix-tests.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
